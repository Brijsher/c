#include<stdio.h>
#include<conio.h>
int main()
{
	int arr[20],i,s,n;
	printf("please input number of elements\n");
	scanf("%d",&n);
	printf("enter %d elements\n",n);
	  for(i=0;i<n;i++)
	   scanf("%d",&arr[i]);
	
		printf("enter the number of search\n");
		scanf("%d",&s);
		
		for(i=0;i<n;i++)
		{
			if(arr[i]==s)
			{
			
			printf("%d no location is %d.\n",s,i+1);
			break;
			}
		}
		if(i==n)
		printf("%d is not located here ",s);
		getch();
}
